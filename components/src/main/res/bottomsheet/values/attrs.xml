<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="AndesBottomSheet" tools:ignore="ResourceName">
        <attr name="andesBottomSheetPeekHeight" format="dimension"/>
        <attr name="andesBottomSheetState" format="enum">
            <enum name="collapsed" value="1000"/>
            <enum name="expanded" value="1001"/>
        </attr>

        <attr name="andesBottomSheetTitleText" format="string"/>
        <attr name="andesBottomSheetTitleAlign" format="enum">
            <enum name="centered" value="2000"/>
            <enum name="left_aligned" value="2001"/>
        </attr>
    </declare-styleable>
</resources>